<p id="notice"><%= notice %></p>

<h1>Demoimgs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demoimgs.each do |demoimg| %>
      <tr>
        <td><%= demoimg.name %></td>
        <td><%= demoimg.img %></td>
        <td><%= link_to 'Show', demoimg %></td>
        <td><%= link_to 'Edit', edit_demoimg_path(demoimg) %></td>
        <td><%= link_to 'Destroy', demoimg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demoimg', new_demoimg_path %>
